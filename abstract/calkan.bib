@Article{Alkan2007,
  Title                    = {Organization and evolution of primate centromeric {DNA} from whole-genome shotgun sequence data.},
  Author                   = {Can Alkan and Mario Ventura and Nicoletta Archidiacono and Mariano Rocchi and S. Cenk Sahinalp and Evan E Eichler},
