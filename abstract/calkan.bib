@Article{Simpson2009,
  Title                    = {{ABySS}: a parallel assembler for short read sequence data.},
  Author                   = {Jared T Simpson and Kim Wong and Shaun D Jackman and Jacqueline E Schein and Steven J M Jones and Inanç Birol},
  Journal                  = {Genome Res},
